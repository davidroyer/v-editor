{
  "name": "v-editor",
  "version": "0.0.2",
  "description": "Vue wrapper for new CK Editor5",
  "repository": {
    "url": "davidroyer/v-editor",
    "type": "git"
  },
  "main": "dist/VEditor.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "test-vue-app",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build",
    "build:umd": "poi build --format cjs --module-name VEditor",
    "build:cjs": "poi build src/VEditor.vue --format cjs --filename.js VEditor.js --no-sourceNap --no-html --no-extractCSS"
  },
  "author": {
    "name": "David Royer",
    "email": "droyer01@gmail.com"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": [
      "cjs",
      "umd"
    ]
  },
  "devDependencies": {
    "bili": "^0.18.2",
    "poi": "^9.3.10",
    "test-vue-app": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.2"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^1.0.0-alpha.1"
  }
}
